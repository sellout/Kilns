(load "library/test-framework")
(load "library/utilities")

{test-result {should {get-length}}
             {test (load "library/simple-components")
                   {include-component "library/strings"}
                   {string-length (tup "test" {rc})}}
             {expected-result {rc 4}}}

{test-result {should {concatenate}}
             {test (load "library/simple-components")
                   {include-component "library/strings"}
                   {string-concat (tup "test " "concat" {rc})}}
             {expected-result {rc "test concat"}}}
