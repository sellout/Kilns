(load "library/test-framework")
(load "library/utilities")

{test-result {should {match-tup-to-par}}
             {test (trigger {test (par {1 ?x} {2 ?y})}
                     {rc (par x y)})
                   {test (tup {a} {b})}}
             {expected-result {rc (par {a} {b})}}}

{test-result {should {match-par-to-tup}}
             {test (trigger {test (tup ?x ?y)}
                     {rc (par x y)})
                   {test (par {1 {a}} {2 {b}})}}
             {expected-result {rc (par {a} {b})}}}
